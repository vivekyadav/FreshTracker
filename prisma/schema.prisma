generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
}

model Item {
  id          Int      @id @default(autoincrement())
  name        String
  category    String?
  quantity    Int      @default(1)
  expiryDate  DateTime?
  addedAt     DateTime @default(now())
  status      String   @default("available") // available, consumed, expired
  imageUrl    String?
}
